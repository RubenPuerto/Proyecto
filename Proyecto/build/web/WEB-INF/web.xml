<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>llenarlista</servlet-name>
        <servlet-class>.llenarlista</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ActionServlet</servlet-name>
        <servlet-class>ActionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ActionServletDetalle</servlet-name>
        <servlet-class>ActionServletDetalle</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ActionServletDetalleTemas</servlet-name>
        <servlet-class>ActionServletDetalleTemas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pruebas</servlet-name>
        <servlet-class>pruebas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerImagen</servlet-name>
        <servlet-class>com.proyecto.Servlet.VerImagen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerImagenDetalle</servlet-name>
        <servlet-class>com.proyecto.Servlet.VerImagenDetalle</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerImgBanner</servlet-name>
        <servlet-class>com.proyecto.Servlet.VerImgBanner</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ContentResibo</servlet-name>
        <servlet-class>com.proyecto.Servlet.ContentResibo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>llenarlista</servlet-name>
        <url-pattern>/llenarlista</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActionServlet</servlet-name>
        <url-pattern>/ActionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActionServletDetalle</servlet-name>
        <url-pattern>/ActionServletDetalle</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActionServletDetalleTemas</servlet-name>
        <url-pattern>/ActionServletDetalleTemas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pruebas</servlet-name>
        <url-pattern>/pruebas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerImagen</servlet-name>
        <url-pattern>/VerImagen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerImagenDetalle</servlet-name>
        <url-pattern>/VerImagenDetalle</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerImgBanner</servlet-name>
        <url-pattern>/VerImgBanner</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ContentResibo</servlet-name>
        <url-pattern>/ContentResibo</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Home.jsp</welcome-file>
    </welcome-file-list>
</web-app>
